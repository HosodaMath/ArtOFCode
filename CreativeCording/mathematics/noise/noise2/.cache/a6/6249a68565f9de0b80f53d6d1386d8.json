t renderTriangle = (x: number, y: number, r: number) => {\r\n    p.push();\r\n    p.translate(x, y);\r\n    p.rotate(-90);\r\n    p.beginShape();\r\n    for (let count = 0; count < 3; count++) {\r\n      //p.fill(p.noise(x * y * 0.01) * 360.0, 80.0, 90.0, 100.0);\r\n      p.vertex(p.cos((360 * count) / 3) * r, p.sin((360 * count) / 3) * r);\r\n    }\r\n    p.endShape(p.CLOSE);\r\n    p.pop();\r\n  };\r\n\r\n  p.draw = () => {\r\n    p.background(220, 220, 250);\r\n    const SIZE = 5;\r\n    p.push();\r\n    p.translate(p.width * 0.5, p.height * 0.5);\r\n    p.beginShape();\r\n    for (let x = -p.width / 2; x < p.width / 2; x += SIZE) {\r\n      for (let y = -p.height / 2; y < p.height / 2; y += SIZE) {\r\n        p.fill(p.noise(x * y * 0.005) * 360.0, 80.0, 100.0, 100.0);\r\n        renderTriangle(x, y, SIZE);\r\n      }\r\n    }\r\n    p.endShape();\r\n    p.pop();\r\n  };\r\n};\r\n\r\nnew p5(sketch);\r\n"},"lineCount":null}},"error":null,"hash":"38e2505a161355b4466ee3042cbd9144","cacheData":{"env":{}}}